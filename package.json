    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "fable-loader": "^1.0.0",
    "google-closure-compiler": "20170423.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-fable": "^1.0.3",
    "webpack": "^2.5.1"
  }
